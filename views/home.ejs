<!DOCTYPE html>
<html lang="en" dir="ltr">
    <head>
        <meta charset="utf-8">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet">
        <link rel="stylesheet" href="./styles/main.css">
        <script src="script.js" charset="utf-8" defer></script>
        <title>Injustice Character Tracker</title>
    </head>
    <body>
        <h1 id="title">Injustice Character Tracker</h1>
        <table id="character-list">
            <thead>
                <tr>
                    <th>Name</th>
                </tr>
            </thead>
            <tbody>
                <% characters.forEach((character) => { %>
                    <tr id="<%- character.name.toLowerCase() %>-<%- character.version.toLowerCase() %>-row">
                        <!-- <td class="character-obtained"><input class="checkbox" type="checkbox" /></td> -->
                        <td class="character-name"><%- character.name %>/<%- character.version %></td>
                        <!-- <td class="character-level">60</td>
                        <td class="character-promotion">X</td> --> %>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </body>
</html>
